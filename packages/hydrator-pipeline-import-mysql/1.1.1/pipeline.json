{
  "resources": {
    "memoryMB": 512,
    "virtualCores": 1
  },
  "driverResources": {
    "memoryMB": 512,
    "virtualCores": 1
  },
  "connections": [
    {
      "from": "Database",
      "to": "Table"
    }
  ],
  "comments": [],
  "postActions": [],
  "stages": [
    {
      "name": "Database",
      "plugin": {
        "name": "Database",
        "type": "batchsource",
        "label": "Database",
        "artifact": {
          "name": "database-plugins",
          "version": "1.6.1",
          "scope": "SYSTEM"
        },
        "properties": {
          "enableAutoCommit": "false",
          "columnNameCase": "No change",
          "jdbcPluginName": "mysql",
          "jdbcPluginType": "jdbc",
          "importQuery": "SELECT * FROM ${table_name}",
          "referenceName": "Database",
          "numSplits": "1"
        }
      }
    },
    {
      "name": "Table",
      "plugin": {
        "name": "Table",
        "type": "batchsink",
        "label": "Table",
        "artifact": {
          "name": "core-plugins",
          "version": "1.6.1",
          "scope": "SYSTEM"
        },
        "properties": {}
      }
    }
  ],
  "schedule": "0 * * * *",
  "engine": "mapreduce",
  "description": "Import Mysql table into HBase"
}
