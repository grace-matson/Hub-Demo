            "outputSchema": "{\"type\":\"record\",\"name\":\"etlSchemaBody\",\"fields\":[{\"name\":\"id\",\"type\":\"int\"},{\"name\":\"currency\",\"type\":\"string\"},{\"name\":\"price\",\"type\":\"float\"},{\"name\":\"num\",\"type\":\"int\"}]}",
