    "stages": [
        {
            "name": "ProductCatalog",
            "plugin": {
                "name": "Stream",
                "type": "batchsource",
                "label": "ProductCatalog",
                "artifact": {
                    "name": "core-plugins",
                    "version": "1.5.0",
                    "scope": "SYSTEM"
                },
                "properties": {
                    "schema": "{\"type\":\"record\",\"name\":\"etlSchemaBody\",\"fields\":[{\"name\":\"id\",\"type\":\"int\"},{\"name\":\"currency\",\"type\":\"string\"},{\"name\":\"price\",\"type\":\"float\"},{\"name\":\"num\",\"type\":\"int\"}]}",
                    "format": "csv",
