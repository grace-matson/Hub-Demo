            "outputSchema": "{\"type\":\"record\",\"name\":\"etlSchemaBody\",\"fields\":[{\"name\":\"id\",\"type\":\"long\"},{\"name\":\"currency\",\"type\":\"string\"},{\"name\":\"price\",\"type\":\"double\"},{\"name\":\"stock\",\"type\":\"int\"}]}",
