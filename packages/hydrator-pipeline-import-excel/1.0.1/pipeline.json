{
    "resources": {
      "memoryMB": 512,
      "virtualCores": 1
    },
    "driverResources": {
      "memoryMB": 512,
      "virtualCores": 1
    },
    "connections": [
      {
        "from": "Excel",
        "to": "TPFSAvro"
      }
    ],
    "comments": [],
    "postActions": [],
    "stages": [
      {
        "name": "Excel",
        "plugin": {
          "name": "Excel",
          "type": "batchsource",
          "label": "Excel",
          "artifact": {
            "name": "core-plugins",
            "version": "1.5.1",
            "scope": "SYSTEM"
          },
          "properties": {
            "reprocess": "false",
            "sheet": "Sheet Name",
            "skipFirstRow": "false",
            "terminateIfEmptyRow": "false",
            "ifErrorRecord": "Ignore error and continue",
            "referenceName": "Excel",
            "filePattern": ".*"
          }
        }
      },
      {
        "name": "TPFSAvro",
        "plugin": {
          "name": "TPFSAvro",
          "type": "batchsink",
          "label": "TPFSAvro",
          "artifact": {
            "name": "core-plugins",
            "version": "1.5.1",
            "scope": "SYSTEM"
          },
          "properties": {}
        }
      }
    ],
    "schedule": "0 * * * *",
    "engine": "mapreduce",
    "description": "Imports Data from Excel file and dumps it to time partitioned file set backed by Hive"
  }
