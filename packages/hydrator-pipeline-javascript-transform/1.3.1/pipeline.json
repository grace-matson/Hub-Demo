{
  "resources": {
    "memoryMB": 512,
    "virtualCores": 1
  },
  "driverResources": {
    "memoryMB": 512,
    "virtualCores": 1
  },
  "connections": [
    {
      "from": "File",
      "to": "CSVParser"
    },
    {
      "from": "CSVParser",
      "to": "CreateID"
    },
    {
      "from": "CreateID",
      "to": "GCS"
    }
  ],
  "comments": [],
  "postActions": [],
  "stages": [
    {
      "name": "File",
      "plugin": {
        "name": "File",
        "type": "batchsource",
        "label": "File",
        "artifact": {
          "name": "core-plugins",
          "version": "[1.7.0-SNAPSHOT, 3.0.0-SNAPSHOT)",
          "scope": "SYSTEM"
        },
        "properties": {
          "referenceName": "Contacts"
        }
      }
    },
    {
      "name": "CSVParser",
      "plugin": {
        "name": "CSVParser",
        "type": "transform",
        "label": "CSVParser",
        "artifact": {
          "name": "transform-plugins",
          "version": "[1.7.0-SNAPSHOT, 3.0.0-SNAPSHOT)",
          "scope": "SYSTEM"
        },
        "properties": {
          "format": "DEFAULT",
          "schema": "{\"type\":\"record\",\"name\":\"etlSchemaBody\",\"fields\":[{\"name\":\"first_name\",\"type\":\"string\"},{\"name\":\"last_name\",\"type\":\"string\"},{\"name\":\"address\",\"type\":\"string\"},{\"name\":\"city\",\"type\":\"string\"},{\"name\":\"state\",\"type\":\"string\"},{\"name\":\"zip\",\"type\":\"int\"}]}",
          "field": "body"
        }
      }
    },
    {
      "name": "CreateID",
      "plugin": {
        "name": "JavaScript",
        "type": "transform",
        "label": "CreateID",
        "artifact": {
          "name": "core-plugins",
          "version": "[1.7.0-SNAPSHOT, 3.0.0-SNAPSHOT)",
          "scope": "SYSTEM"
        },
        "properties": {
          "script": "function transform(input, emitter, context) {\n  id  = input.first_name + input.last_name;\n  input.id = id;\n  emitter.emit(input);\n}",
          "schema": "{\"type\":\"record\",\"name\":\"etlSchemaBody\",\"fields\":[{\"name\":\"id\",\"type\":\"string\"},{\"name\":\"first_name\",\"type\":\"string\"},{\"name\":\"last_name\",\"type\":\"string\"},{\"name\":\"address\",\"type\":\"string\"},{\"name\":\"city\",\"type\":\"string\"},{\"name\":\"state\",\"type\":\"string\"},{\"name\":\"zip\",\"type\":\"int\"}]}"
        }
      }
    },
    {
        "name": "GCS",
        "plugin": {
            "name": "GCS",
            "type": "batchsink",
            "label": "Google Cloud Storage",
            "artifact": {
                "name": "google-cloud",
                "version": "[0.12.0,1.0.0-SNAPSHOT)",
                "scope": "SYSTEM"
            },
            "properties": {
          "schema": "{\"type\":\"record\",\"name\":\"etlSchemaBody\",\"fields\":[{\"name\":\"id\",\"type\":\"string\"},{\"name\":\"first_name\",\"type\":\"string\"},{\"name\":\"last_name\",\"type\":\"string\"},{\"name\":\"address\",\"type\":\"string\"},{\"name\":\"city\",\"type\":\"string\"},{\"name\":\"state\",\"type\":\"string\"},{\"name\":\"zip\",\"type\":\"int\"}]}"
            }
        }
    }
  ],
  "schedule": "0 * * * *",
  "engine": "mapreduce",
  "description": "Read contact data from file, parse data, create id based on first and last name, and write to HBase with id as the key."
}
