{
  "resources": {
    "memoryMB": 512,
    "virtualCores": 1
  },
  "driverResources": {
    "memoryMB": 512,
    "virtualCores": 1
  },
  "connections": [
    {
      "from": "FileSource",
      "to": "OneMinuteWindow"
    },
    {
      "from": "OneMinuteWindow",
      "to": "LogParser"
    },
    {
      "from": "LogParser",
      "to": "GroupByAggregate"
    },
    {
      "from": "GroupByAggregate",
      "to": "TPFSAvro"
    },
    {
      "from": "CreateIndex",
      "to": "ElasticsearchSink"
    },
    {
      "from": "LogParser",
      "to": "CreateIndex"
    }
  ],
  "comments": [],
  "postActions": [],
  "stages": [
    {
      "name": "FileSource",
      "plugin": {
        "name": "File",
        "type": "streamingsource",
        "label": "FileSource",
        "artifact": {
          "name": "spark-plugins",
          "version": "1.6.0",
          "scope": "SYSTEM"
        },
        "properties": {
          "schema": "{\"type\":\"record\",\"name\":\"etlSchemaBody\",\"fields\":[{\"name\":\"body\",\"type\":\"string\"}]}",
          "format": "text",
          "extensions": "txt",
          "referenceName": "FileSource"
        }
      }
    },
    {
      "name": "OneMinuteWindow",
      "plugin": {
        "name": "Window",
        "type": "windower",
        "label": "OneMinuteWindow",
        "artifact": {
          "name": "core-plugins",
          "version": "1.6.0",
          "scope": "SYSTEM"
        },
        "properties": {
          "width": "60",
          "slideInterval": "60"
        }
      }
    },
    {
      "name": "LogParser",
      "plugin": {
        "name": "LogParser",
        "type": "transform",
        "label": "LogParser",
        "artifact": {
          "name": "core-plugins",
          "version": "1.6.0",
          "scope": "SYSTEM"
        },
        "properties": {
          "logFormat": "CLF",
          "inputName": "body"
        }
      }
    },
    {
      "name": "GroupByAggregate",
      "plugin": {
        "name": "GroupByAggregate",
        "type": "batchaggregator",
        "label": "GroupByAggregate",
        "artifact": {
          "name": "core-plugins",
          "version": "1.6.0",
          "scope": "SYSTEM"
        },
        "properties": {
          "groupByFields": "httpStatus,ip",
          "aggregates": "ipCount:Count(ip)"
        }
      }
    },
    {
      "name": "TPFSAvro",
      "plugin": {
        "name": "TPFSAvro",
        "type": "batchsink",
        "label": "TPFSAvro",
        "artifact": {
          "name": "core-plugins",
          "version": "1.6.0",
          "scope": "SYSTEM"
        },
        "properties": {
          "schema": "{\"type\":\"record\",\"name\":\"etlSchemaBody\",\"fields\":[{\"name\":\"httpStatus\",\"type\":\"int\"},{\"name\":\"ip\",\"type\":\"string\"},{\"name\":\"ipCount\",\"type\":\"long\"}]}",
          "name": "tpfsAvroSink"
        }
      }
    },
    {
      "name": "CreateIndex",
      "plugin": {
        "name": "JavaScript",
        "type": "transform",
        "label": "CreateIndex",
        "artifact": {
          "name": "core-plugins",
          "version": "1.6.0",
          "scope": "SYSTEM"
        },
        "properties": {
          "script": "function transform(input, emitter, context) {\n  var obj = {\n    id: input.ip + \":\" + input.uri,\n    uri: input.uri,\n    ip: input.ip,\n    browser: input.browser,\n    device: input.device,\n    httpStatus: input.httpStatus,\n    ts: input.ts\n  };\n  \n  emitter.emit(obj);\n}",
          "schema": "{\"type\":\"record\",\"name\":\"etlSchemaBody\",\"fields\":[{\"name\":\"id\",\"type\":\"string\"},{\"name\":\"uri\",\"type\":\"string\"},{\"name\":\"ip\",\"type\":\"string\"},{\"name\":\"browser\",\"type\":\"string\"},{\"name\":\"device\",\"type\":\"string\"},{\"name\":\"httpStatus\",\"type\":\"int\"},{\"name\":\"ts\",\"type\":\"long\"}]}"
        }
      }
    },
    {
      "name": "ElasticsearchSink",
      "plugin": {
        "name": "Elasticsearch",
        "type": "batchsink",
        "label": "ElasticsearchSink",
        "artifact": {
          "name": "elasticsearch-plugins",
          "version": "1.6.0",
          "scope": "USER"
        },
        "properties": {
          "referenceName": "ElasticsearchSink",
          "es.host": "http://${eshost}:${esport}",
          "es.index": "logdata",
          "es.type": "data",
          "es.idField": "id"
        }
      }
    }
  ],
  "batchInterval": "1m",
  "description": "Ingesting access log from realtime file source and compute ip and http status code analytics and further ship the logs to elastic for on demand search and analytics"
}
